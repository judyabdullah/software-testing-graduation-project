		{
			"name": "search_product API",
			"event": [
				{
					"listen": "test",
					"script": {
						"exec": [
							"// Check status code\r",
							"pm.test(\"Status code is 200\", function () {\r",
							"    pm.response.to.have.status(200);\r",
							"});\r",
							"\r",
							"// Validate API response code inside JSON\r",
							"pm.test(\"Response code is 200 inside body\", function () {\r",
							"    var json = pm.response.json();\r",
							"    pm.expect(json.responseCode).to.eql(200);\r",
							"});\r",
							"\r",
							"// Check that products list is returned\r",
							"pm.test(\"Products list is returned\", function () {\r",
							"    var json = pm.response.json();\r",
							"    pm.expect(json.products.length).to.be.above(0);\r",
							"});\r",
							"\r",
							"// Validate product structure\r",
							"pm.test(\"First product has required fields\", function () {\r",
							"    var json = pm.response.json();\r",
							"    \r",
							"    pm.expect(json.products[0]).to.have.property(\"id\");\r",
							"    pm.expect(json.products[0]).to.have.property(\"name\");\r",
							"    pm.expect(json.products[0]).to.have.property(\"price\");\r",
							"});"
						],
						"type": "text/javascript",
						"packages": {},
						"requests": {}
					}
				}
